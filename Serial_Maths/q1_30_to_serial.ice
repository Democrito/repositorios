        {
          "id": "5f5a104b-ce08-44c5-bd43-dc6ae0b58377",
          "type": "basic.code",
          "data": {
            "ports": {
              "in": [
                {
                  "name": "clk"
                },
                {
                  "name": "rst"
                },
                {
                  "name": "number_in",
                  "range": "[31:0]",
                  "size": 32
                },
                {
                  "name": "input_valid"
                }
              ],
              "out": [
                {
                  "name": "sign"
                },
                {
                  "name": "bcd_int",
                  "range": "[3:0]",
                  "size": 4
                },
                {
                  "name": "bcd_frac",
                  "range": "[35:0]",
                  "size": 36
                },
                {
                  "name": "_done"
                }
              ]
            },
            "params": [],
            "code": "    // module Q1.30 with sign to 40 bits BCD with sign (1 bit sign + 1 BCD integer digit + 9 BCD fractionary digits) \r\n    \r\n    // Definición de los estados de la máquina de estados\r\n    localparam [1:0]\r\n        IDLE       = 0,  // Estado de espera\r\n        PROCESSING = 1,  // Estado de procesamiento de la fracción\r\n        DONE       = 2;  // Estado de finalización\r\n    \r\n    reg [1:0] state, next_state;  // Registros de estados\r\n    \r\n    // Registros de salida\r\n    reg        sign_out;        // Bit de signo (0 para positivo, 1 para negativo)\r\n    reg [3:0]  integer_digit;   // Dígito entero en BCD (0 o 1)\r\n    reg [35:0] fraction_digits; // Nueve dígitos fraccionarios en BCD (4 bits por dígito)\r\n    reg        output_valid;    // Indica que la salida es válida\r\n    \r\n    // Asignación de los registros anteriores como cables de salida \r\n    assign sign     = sign_out;\r\n    assign bcd_int  = integer_digit;\r\n    assign bcd_frac = fraction_digits; // Se le suma 1 para tener 9 dígitos exactos, por las pérdidas en los trucamientos.\r\n    assign _done    = output_valid;\r\n\r\n    // Registros internos\r\n    reg [35:0] bcd_digits_reg;     // Registro para acumular los dígitos BCD\r\n    reg [33:0] temp;               // Registro temporal para los cálculos\r\n    reg [29:0] fraction_reg;       // Registro para la parte fraccionaria\r\n    reg  [3:0] digit_counter;      // Contador de dígitos procesados\r\n    reg  [3:0] bcd_digit;\r\n\r\n    // Máquina de estados\r\n    always @(posedge clk or posedge rst) begin\r\n        if (rst) begin\r\n            state <= IDLE;\r\n            sign_out <= 0;\r\n            integer_digit <= 0;\r\n            fraction_digits <= 0;\r\n            output_valid <= 0;\r\n            fraction_reg <= 0;\r\n            digit_counter <= 0;\r\n            bcd_digits_reg <= 0;\r\n        end else begin\r\n            state <= next_state;\r\n            case (state)\r\n                IDLE: begin\r\n                    output_valid <= 0;\r\n                    if (input_valid) begin\r\n                        // Captura inicial\r\n                        sign_out <= number_in[31];                    // Bit de signo\r\n                        integer_digit <= number_in[30];               // Dígito entero (0 o 1)\r\n                        fraction_reg <= number_in[29:0] + 1;          // Parte fraccionaria \"+ 1\" para ser exactos.\r\n                        digit_counter <= 0;\r\n                        bcd_digits_reg <= 0;\r\n                        next_state <= PROCESSING;\r\n                    end else begin\r\n                        next_state <= IDLE;\r\n                    end\r\n                end\r\n                PROCESSING: begin\r\n                    if (digit_counter < 9) begin\r\n                        // Multiplicación por 10 en aritmética de punto fijo\r\n                        temp            = {4'b0, fraction_reg} * 10;  // Resultado temporal de 34 bits\r\n                        // Extraer el dígito BCD (bits 33:30)\r\n                        bcd_digit       = temp[33:30];\r\n                        // Actualizar la fracción (bits 29:0)\r\n                        fraction_reg   <= temp[29:0];\r\n                        // Acumular el dígito BCD desplazando a la izquierda\r\n                        bcd_digits_reg <= (bcd_digits_reg << 4) | {28'b0, bcd_digit};\r\n                        digit_counter  <= digit_counter + 1;\r\n                        next_state     <= PROCESSING;\r\n                    end else begin\r\n                        next_state <= DONE;\r\n                    end\r\n                end\r\n                DONE: begin\r\n                    fraction_digits <= bcd_digits_reg; // Asignar el resultado final\r\n                    output_valid    <= 1;              // Indicar que la salida es válida\r\n                    next_state      <= IDLE;\r\n                end\r\n                default: begin\r\n                    next_state      <= IDLE;\r\n                end\r\n            endcase\r\n        end\r\n    end\r\n    \r\n    "
          },
